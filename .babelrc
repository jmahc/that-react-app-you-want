{
  "presets": [
    [
      "es2015",
      {
        "loose": true
      }
    ],
    [
      "env",
      {
        "browsers": "last 1 Chrome version",
        "loose": true,
        "modules": false,
        "targets": {
          "node": "8.0"
        },
        "useBuiltIns": true
      }
    ],
    "stage-0",
    "react",
    "babili"
  ],
  "comments": false,
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
