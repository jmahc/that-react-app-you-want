{
  "name": "that-react-app-you-want",
  "version": "0.2.2",
  "author": {
    "name": "Jordan McArdle",
    "email": "jordan@mcardle.tech",
    "url": "https://github.com/jmahc/"
  },
  "description": "That react app you always wanted: webpack@3.4.0, react, postCSS, purifycss, dll's and code splitting examples.  That includes prefetching. Highly opinionated but you better like it.",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=0.27.5"
  },
  "scripts": {
    "build": "rimraf ./dist/* && cross-env NODE_ENV=production webpack --profile --progress --env production",
    "clean-all": "rm -rf ./dist/* ./dll/* node_modules package-lock.json yarn.lock .yarnclean ./**/*.DS_STORE ./.DS_STORE",
    "clean-dist": "rimraf ./dist/*",
    "clean-dll": "rimraf ./dll/*",
    "clean-npm": "rm -rf node_modules package-lock.json",
    "clean-yarn": "rm -rf node_modules yarn.lock .yarnclean",
    "dev": "yarn run start",
    "dev-bundlebuddy": "bundle-buddy ./dist/*.map",
    "dev-gen": "generact",
    "dev-vendor": "rm -rf ./dll/* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "format": "prettier-standard 'src/**/*.js'",
    "postmerge": "yarn",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start": "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dashboard --color red --title jmahc --minimal -- webpack-dev-server --env development --open",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chunk-manifest-webpack-plugin": "^1.1.1",
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
