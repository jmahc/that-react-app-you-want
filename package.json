  "description":
    "That react app you always wanted: webpack@3.10.0, react@16.0.0, postCSS, purifycss, DLL's and code splitting examples. That includes prefetching. Highly opinionated but you better like it.",
  "main": "src/index.js",
  "files": ["dist", "dll"],
  "homepage": "https://github.com/jmahc/that-react-app-you-want#readme",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "build":
      "rimraf ./dist/* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "build:win":
      "del /q dist\\* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "clean": "rm -rf ./dist/* ./dll/* node_modules ./**/*.DS_STORE ./.DS_STORE",
    "clean-all": "npm run clean",
    "clean:all": "del /q dist\\* dll\\* node_modules",
    "clean-dist": "rm -rf ./dist/*",
    "clean:dist": "del /q dist\\*",
    "clean-dll": "del /q dll\\*",
    "dev":
      "rm -rf ./dll/* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "dev:win":
      "del /q dll\\* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "format": "prettier-standard 'src/**/*.js'",
    "postmerge": "npm install",
    "postinstall":
      "cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start":
      "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dev-server --env development --open",
    "webpack-defaults": "webpack-defaults"
  },
