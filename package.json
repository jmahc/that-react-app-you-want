{
  "name": "that-react-app-you-want",
  "version": "0.1.0",
  "description": "That react app you always wanted: webpack@3.1.0, react, postCSS, purifycss, dll's and code splitting examples. Highly opinionated but you better like it.",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf ./dist/* && cross-env NODE_ENV=production webpack --env production",
    "clean:dll": "rm -rf ./dll/*",
    "clean:npm": "rm -rf node_modules package-lock.json",
    "clean:yarn": "rm -rf node_modules yarn.lock",
    "dev": "yarn run start",
    "dev:bundlebuddy": "bundle-buddy ./dist/*.map",
    "dev:gen": "generact",
    "dev:vendor": "rm -rf ./dll/* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged",
    "postmerge": "yarn",
    "start": "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dashboard --color red --title jmahc --minimal -- webpack-dev-server --env development --open"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "bundle-buddy": "^0.1.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "generact": "^0.2.0",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "prettier-eslint": "^6.4.2",
    "prettier-standard": "^6.0.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redbox-react": "^1.4.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dashboard": "^1.0.0-2",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "promise": "^7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "lint-staged": {
    "src/**/*.js": ["prettier-standard", "git add"],
    "src/**/*.jsx": ["prettier-standard", "git add"]
  },
  "browser": {
    "buffer": "none"
  },
  "engines": {
    "node": ">=8.1.3",
    "npm": ">=5.0.3",
    "yarn": ">=0.27.5"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/jmahc/that-react-app-you-want/"
  },
  "keywords": [
    "babel",
    "babili",
    "chunks",
    "code-splitting",
    "DLL",
    "DllPlugin",
    "DllReferencePlugin",
    "es6",
    "eslint",
    "generact",
    "hot-loader",
    "jsx",
    "manifest",
    "postCSS",
    "purifycss",
    "react",
    "tree-shaking",
    "webpack",
    "webpackDLL",
    "webpack3",
    "webpack-3",
    "webpack-v3",
    "webpack@3.0.0",
    "webpack@3.1.0"
  ],
  "author": {
    "name": "Jordan McArdle",
    "email": "jordan@mcardle.tech",
    "url": "http://mcardle.tech/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmahc/that-react-app-you-want/issues"
  },
  "homepage": "https://github.com/jmahc/that-react-app-you-want#readme"
}
