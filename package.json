  "dependencies": {
    "emotion": "^9.2.12",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-emotion": "^9.2.12"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ],
    ".storybook/**/*.js": [
      "prettier --config ./.prettierrc.js --write \"src/**/*.js\"",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --config ./.prettierrc.js --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/jmahc/that-react-app-you-want/"
  },
  "bugs": {
