    "redbox-react": "^1.4.3",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-chunk-hash": "^0.4.0",
