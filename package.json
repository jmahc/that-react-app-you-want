{
  "name": "that-react-app-you-want",
  "version": "0.2.20",
  "description": "That react app you always wanted.",
  "main": "dist/index.js",
  "homepage": "https://github.com/jmahc/that-react-app-you-want#readme",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "build:win": "del /q dist\\* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "clean": "rm -rf ./dist/* ./dll/* node_modules ./**/*.DS_STORE ./.DS_STORE",
    "clean-all": "npm run clean",
    "clean:all": "del /q dist\\* dll\\* node_modules",
    "clean-dist": "rm -rf ./dist/*",
    "clean:dist": "del /q dist\\*",
    "clean-dll": "del /q dll\\*",
    "dev": "cross-env NODE_ENV=development webpack --profile --progress --env vendors --config ./webpack.config.babel.js",
    "format": "prettier-standard 'src/**/*.js'",
    "postmerge": "npm install",
    "postinstall:later": "npm run dev",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start": "cross-env NODE_ENV=development webpack-dev-server --env development --open",
    "start:dev": "cross-env NODE_ENV=development webpack --profile --progress --env development --config ./webpack.config.babel.js",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
