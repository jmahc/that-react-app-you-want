{
  "name": "that-react-app-you-want",
  "version": "0.2.9",
  "author": {
    "name": "Jordan McArdle",
    "email": "jordan@mcardle.tech",
    "url": "https://github.com/jmahc/"
  },
  "description": "That react app you always wanted: webpack@3.7.0, react@16.0.0, postCSS, purifycss, DLL's and code splitting examples. That includes prefetching. Highly opinionated but you better like it.",
  "main": "src/index.js",
  "files": [
    "dist",
    "dll"
  ],
  "homepage": "https://github.com/jmahc/that-react-app-you-want#readme",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "build": "rimraf ./dist/* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "build:win": "del /q dist\\* && cross-env NODE_ENV=production webpack -p --profile --progress --env production --config ./webpack.config.babel.js",
    "clean": "rm -rf ./dist/* ./dll/* node_modules yarn.lock .yarnclean yarn-error.log ./**/*.DS_STORE ./.DS_STORE",
    "clean-all": "yarn run clean",
    "clean:all": "del /q dist\\* dll\\* node_modules yarn.lock .yarnclean",
    "clean-dist": "rm -rf ./dist/*",
    "clean:dist": "del /q dist\\*",
    "clean-dll": "del /q dll\\*",
    "clean-yarn": "rm -rf node_modules yarn.lock .yarnclean",
    "clean:yarn": "del /q node_modules yarn.lock .yarnclean",
    "dev": "yarn run start",
    "dev-vendor": "rm -rf ./dll/* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "dev:vendor": "del /q dll\\* && cross-env NODE_ENV=development webpack --profile --progress --env development --config ./config/webpack.config.vendor.babel.js",
    "format": "prettier-standard 'src/**/*.js' && prettier-standard 'src/**/*.jsx'",
    "postmerge": "yarn",
    "postinstall": "# TODO clear the persistent caching with the `cache-loader`",
    "precommit": "lint-staged",
    "release": "standard-version",
    "start": "cross-env NODE_ENV=development PORT_NUMBER=8080 webpack-dev-server --env development --open",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "generact": "^0.3.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "lint-staged": "^4.2.1",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss": "^6.0.11",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.0",
    "prettier-standard": "^7.0.1",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^3.0.0",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
